<?xml version="1.0" encoding="UTF-8"?>

<?import bms.player.beatoraja.launcher.NumericSpinner?>
<?import bms.player.beatoraja.launcher.EditableTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SpinnerValueFactory.DoubleSpinnerValueFactory?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Tooltip?>

<?import javafx.scene.control.CheckBox?>

<?import javafx.scene.layout.Region?>
<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bms.player.beatoraja.launcher.ResourceConfigurationView">
	<HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
		<VBox.margin>
			<Insets bottom="20.0" />
		</VBox.margin>
		<Label prefHeight="101.0" prefWidth="100.0" text="%BMS_Path"
			HBox.hgrow="ALWAYS">
			<HBox.margin>
				<Insets left="10.0" />
			</HBox.margin>
		</Label>
		<ListView fx:id="bmsroot"
			onDragDropped="#songPathDragDropped" onDragOver="#onSongPathDragOver"
			prefHeight="100.0" prefWidth="500.0" HBox.hgrow="ALWAYS">
			<HBox.margin>
				<Insets top="10.0" />
			</HBox.margin>
		</ListView>
		<VBox prefHeight="162.0" prefWidth="150.0">
			<Button mnemonicParsing="false" onAction="#addSongPath"
				prefHeight="31.0" prefWidth="150" text="%+">
				<VBox.margin>
					<Insets left="10.0" right="10.0" top="10.0" />
				</VBox.margin>
			</Button>
			<Button mnemonicParsing="false" onAction="#removeSongPath"
				prefHeight="31.0" prefWidth="150" text="%-">
				<VBox.margin>
					<Insets left="10.0" right="10.0" top="10.0" />
				</VBox.margin>
			</Button>
			<Button mnemonicParsing="false" onAction="#markAsDownloadDirectory"
					prefHeight="31.0" prefWidth="150" text="%MARK_AS_DOWNLOAD_DIRECTORY">
				<tooltip><Tooltip text="%MARK_AS_DOWNLOAD_DIRECTORY_HINT" /></tooltip>
				<VBox.margin>
					<Insets left="10.0" right="10.0" top="10.0" />
				</VBox.margin>
			</Button>
		</VBox>
	</HBox>
	<HBox prefHeight="100.0" prefWidth="200.0"
		AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
		AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
		VBox.vgrow="ALWAYS">
		<Label prefHeight="166.0" prefWidth="100.0" text="%Table_URL"
			HBox.hgrow="ALWAYS">
			<HBox.margin>
				<Insets left="10.0" />
			</HBox.margin>
		</Label>
		<VBox prefHeight="100.0" prefWidth="536.0" HBox.hgrow="ALWAYS">
			<HBox>
				<Region minWidth="5.0" prefHeight="28.0" />
				<Label text="Active" style="-fx-font-size: 14px; -fx-font-weight: bold;" />
				<Region minWidth="5.0" prefHeight="28.0" />
				<TextField fx:id="url" prefHeight="28.0" minWidth="338.0" HBox.hgrow="ALWAYS"/>
				<Region minWidth="10.0" prefHeight="28.0" />
				<Button mnemonicParsing="false" onAction="#addTableURL" prefHeight="28.0" prefWidth="80.0" text="%+"/>
				<Region minWidth="15.0" prefHeight="28.0" />
				<Label text="Available" style="-fx-font-size: 14px; -fx-font-weight: bold;" />
				<Region minWidth="334.0" prefHeight="28.0" HBox.hgrow="ALWAYS"/>
			</HBox>
			<HBox VBox.vgrow="ALWAYS">
				<EditableTableView fx:id="tableurl" prefHeight="167.0"
					prefWidth="200.0" HBox.hgrow="ALWAYS">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</EditableTableView>
				<VBox prefHeight="185.0" prefWidth="100.0">
					<Button mnemonicParsing="false" onAction="#removeTableURL"
						prefHeight="31.0" prefWidth="80.0" text="%-">
						<VBox.margin>
							<Insets left="10.0" right="10.0" top="10.0" />
						</VBox.margin>
					</Button>
					<Button mnemonicParsing="false" onAction="#moveTableURLUp"
						prefHeight="31.0" prefWidth="80.0" text="↑">
						<VBox.margin>
							<Insets left="10.0" right="10.0" top="10.0" />
						</VBox.margin>
					</Button>
					<Button mnemonicParsing="false" onAction="#moveTableURLDown"
						prefHeight="31.0" prefWidth="80.0" text="↓">
						<VBox.margin>
							<Insets left="10.0" right="10.0" top="10.0" />
						</VBox.margin>
					</Button>
					<Button mnemonicParsing="false" onAction="#moveTableURLIn"
							prefHeight="31.0" prefWidth="80.0" text="←">
						<VBox.margin>
							<Insets left="10.0" right="10.0" top="10.0" />
						</VBox.margin>
					</Button>
					<Button mnemonicParsing="false" onAction="#moveTableURLOut"
							prefHeight="31.0" prefWidth="80.0" text="→">
						<VBox.margin>
							<Insets left="10.0" right="10.0" top="10.0" />
						</VBox.margin>
					</Button>
				</VBox>
                <EditableTableView fx:id="available_tables" prefHeight="167.0"
                                   prefWidth="200.0" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets top="5.0" right="10.0" />
                    </HBox.margin>
                </EditableTableView>
			</HBox>
			<HBox>
			    <Button mnemonicParsing="false" onAction="#refreshLocalTableInfo" prefHeight="25.0" prefWidth="190.0" text="%TABLES_CHECK_STATUS">
			        <HBox.margin>
			            <Insets top="10.0" right="10.0" />
			        </HBox.margin>
			    </Button>
			    <Button mnemonicParsing="false" onAction="#loadNewTables" prefHeight="25.0" prefWidth="190.0" text="%TABLES_LOAD_NEW">
			        <HBox.margin>
			            <Insets top="10.0" right="10.0" />
			        </HBox.margin>
			    </Button>
			    <Button mnemonicParsing="false" onAction="#loadSelectedTables" prefHeight="25.0" prefWidth="190.0" text="%TABLES_LOAD_SELECTED">
			        <HBox.margin>
			            <Insets top="10.0" right="10.0" />
			        </HBox.margin>
			    </Button>
			    <Button mnemonicParsing="false" onAction="#loadAllTables" prefHeight="25.0" prefWidth="190.0" text="%TABLES_LOAD_ALL">
			        <HBox.margin>
			            <Insets top="10.0" right="10.0" />
			        </HBox.margin>
			    </Button>
			</HBox>
		</VBox>
	</HBox>
	<CheckBox fx:id="updatesong" mnemonicParsing="false"
		prefHeight="25.0" prefWidth="300.0" text="%UPDATE_SONG">
		<VBox.margin>
			<Insets left="10.0" />
		</VBox.margin>
	</CheckBox>
</VBox>

